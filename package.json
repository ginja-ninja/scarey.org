  "bugs": {
    "url": "https://github.com/npbee/scarey.org/issues"
  },
  "homepage": "https://github.com/npbee/scarey.org",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-ruby-sass": "^4.0.0",
    "gulp-uglify": "^3.0.2",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.14.3"
  }
